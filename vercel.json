{
  "builds": [
    {
      "src": "api/app.py",
      "use": "@vercel/python"
    },
    {
        "src": "/ui/build/*",
        "use": "@now/static"
      }
  ],
  "routes": [
    {
      "src": "api/(.*)",
      "dest": "api/app.py"
    },
    {
      "src": "/ui/(.*)",
      "dest": "/ui/build/$1"
    },
    {
      "src": "/ui",
      "dest": "/ui/build/index.html"
    }
  ]
}
