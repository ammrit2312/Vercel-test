{
    "version": 2,
    "builds": [
      {
        "src": "ui/src/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "ui/public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "ui/(.*)",
        "dest": "ui/src/index.js"
      }
    ]
  }