{
  "builds": [
    {
      "src": "api/app.py",
      "use": "@vercel/python"
    },
    {
      "src": "ui/*",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "api/(.*)",
      "dest": "api/app.py"
    },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "ui/build/index.html" }
  ]
}
